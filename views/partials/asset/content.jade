if (sagecell)
  - scripts.push('sage');
  div.sage
    script(type="text/x-sage")
      != sagecell
else if asset.externalUrl && asset.externalUrl.match( /^https?:\/\/www.youtube.com\/watch\?v=/ )
  div.embed-responsive.embed-responsive-16by9        
    iframe.embed-responsive-item(src="https://www.youtube.com/embed/" + asset.externalUrl.split("v=")[1])
else if asset.contentType == "application/pdf"
  div.embed-responsive.embed-responsive-16by9                
    iframe.embed-responsive-item(src="https://docs.google.com/gview?embedded=true&url=" + asset.url(),frameborder="0")
else if(asset.urlThumbnail)
  a.thumbnail(href=asset.url())
    img.embed-responsive-item(src=asset.urlThumbnail)
else if(asset.pngThumbnail)
  a.thumbnail(href=asset.url())
    img.embed-responsive-item(src="/assets/#{asset._id}.png")
else
  div.embed-responsive.embed-responsive-16by9                
    iframe.embed-responsive-item(src=asset.url().replace(/^http:/,'https:'),frameborder="0")
